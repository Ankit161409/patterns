public class Main
{
	public static void main(String[] args) {
	  // Scanner s=new Scanner(System.in);
	   // int x= s.nextInt();  
		 for(int i=1; i<=5; i++)
	 {
	     for(int j=1; j<=5; j++)
	        { if(i>=2 && j>=2 && i<=4 && j<=4){
	         System.out.print(" ");}
	         else
	     { System.out.print("*");
	 }
	} System.out.println();
}
}
}